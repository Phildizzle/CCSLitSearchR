@article{ WOS:000506236000007,
Author = {Burggraaff, Christiaan and Trilling, Damian},
Title = {Through a different gate: An automated content analysis of how online
   news and print news differ},
Journal = {JOURNALISM},
Year = {{2020}},
Volume = {{21}},
