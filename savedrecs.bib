@article{ WOS:000449285600013,
Author = {Venturini, Tommaso and Bounegru, Liliana and Gray, Jonathan and Rogers,
   Richard},
Title = {A reality check(list) for digital methods},
Journal = {NEW MEDIA \& SOCIETY},
Year = {{2018}},
Volume = {{20}},
Number = {{11}},
Pages = {{4195-4217}},
Month = {{NOV}},
Abstract = {{Digital Methods can be defined as the repurposing of the inscriptions
   generated by digital media for the study of collective phenomena. The
   strength of these methods comes from their capacity to take advantage of
   the data and computational capacities of online platforms; their
   weakness comes from the difficulty to separate the phenomena that they
   investigate from the features of the media in which they manifest ('the
   medium is the message', according to McLuhan's 1964 dictum). In this
   article, we discuss various methodological difficulties deriving from
   the lack of separation between medium and message and propose eight
   practical precautions to deal with it.}},
