@article{ WOS:000511821300001,
Author = {Huh, Jisu and Kim, Hyejin and Rath, Bhavtosh and Lu, Xinyu and
   Srivastava, Jaideep},
Title = {You reap where you sow: a trust-based approach to initial seeding for
   viral advertising},
Journal = {INTERNATIONAL JOURNAL OF ADVERTISING},
Year = {{2020}},
Volume = {{39}},
Number = {{7}},
Pages = {{963-989}},
Month = {{OCT 2}},
Abstract = {{This study examines the role of source trust in viral ad diffusion,
   specifically the impact of source trust on the reach and speed of ad
   diffusion. It tests the feasibility of using
   computer-algorithm-generated social media metrics, indicating the degree
   to which each person is trusted by others within a social network, for
   trust-based viral ad seeding strategy, and future research on viral
   advertising. Applying trust theory and the computational trust research
   approach using real-life viral ad cases, this study found that only a
   small proportion of social media users exposed to viral ads tend to
   contribute to ad diffusion, and those with higher trust scores make
   significantly stronger contributions to spreading viral ads faster and
   more broadly. Additionally, individuals' source trust scores have
   stronger impact on the extent and speed of viral ad diffusion,
   especially in the situation where the ads contained
   socially-controversial messages. Theoretical and methodological
   contributions and practical implications are offered.}},
