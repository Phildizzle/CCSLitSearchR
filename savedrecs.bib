@article{ WOS:000429897800008,
Author = {Zamith, Rodrigo},
Title = {A Computational Approach for Examining the Comparability of
   ``Most-Viewed Lists{''} on Online News Sites},
Journal = {JOURNALISM \& MASS COMMUNICATION QUARTERLY},
Year = {{2018}},
Volume = {{95}},
Number = {{1}},
Pages = {{122-141}},
Month = {{MAR}},
Abstract = {{This study introduces a computational approach for evaluating the lists
   of most-viewed items present on the homepages of many news
   organizations, focusing on two dimensions: the list's rate of change
   over the course of the day and the median time it takes a news item to
   appear on the list. That approach is then applied in an analysis of 21
   news organizations over 2 months, revealing clusters across those
   dimensions which indicate the reporting of different data. Scholars are
   ultimately encouraged to perform their own analyses and cautioned
   against assuming the lists are comparable just because they appear
   alike.}},
