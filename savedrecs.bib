@article{ WOS:000507043900001,
Author = {Parks, Perry},
Title = {Toward a humanistic turn for a more ethical journalism},
Journal = {JOURNALISM},
Year = {{2020}},
Volume = {{21}},
Number = {{9}},
Pages = {{1229-1245}},
Month = {{SEP}},
Abstract = {{This article argues that the social scientific epistemology that has
   dominated journalism for the past half-century has devalued the moral
   implications of public affairs and deprived citizens of the ethical
   tools necessary to make humane political decisions. Reviewing the
   contingent history of the integration of journalistic and social
   scientific methods leading to journalism's computational turn, the essay
   calls for a humanistic reconceptualization away from journalists' role
   as political interpreters toward a comparable role as moral
   interpreters.}},
