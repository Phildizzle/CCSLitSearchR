@article{ WOS:000537889300001,
Author = {Haim, Mario and Jungblut, Marc},
Title = {Politicians' Self-depiction and Their News Portrayal: Evidence from 28
   Countries Using Visual Computational Analysis},
Journal = {POLITICAL COMMUNICATION},
Year = {{2021}},
