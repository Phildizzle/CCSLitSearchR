@article{ WOS:000474583000007,
Author = {Venturini, Tommaso and Rogers, Richard},
Title = {``API-Rased Research{''} or How can Digital Sociology and Journalism
   Studies Learn from the Facebook and Cambridge Analytica Data Breach},
Journal = {DIGITAL JOURNALISM},
Year = {{2019}},
Volume = {{7}},
Number = {{4}},
Pages = {{532-540}},
Abstract = {{API-based research is an approach to computational social sciences and
   digital sociology based on the extraction of records from the datasets
   made available by online platforms through their application programming
   interfaces (APIs). This type of research has allowed the collection of
   detailed information on large populations, thereby effectively
   challenging the distinction between qualitative and quantitative
   methods. Lately, access to this source of data has been significantly
   reduced after a series of scandals connected to the use of personal
   information. The closure of APIs, however, can have positive effects, if
   it encourages researchers to reduce their dependence on mainstream
   platforms and explore new sources and ways to collect online records
   that are closer to the digital fieldwork.}},
