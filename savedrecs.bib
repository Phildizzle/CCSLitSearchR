@article{ WOS:000415237000006,
Author = {Vargo, Chris and Guo, Lei},
Title = {Networks, Big Data, and Intermedia Agenda Setting: An Analysis of
   Traditional, Partisan, and Emerging Online US News},
Journal = {JOURNALISM \& MASS COMMUNICATION QUARTERLY},
Year = {{2017}},
Volume = {{94}},
Number = {{4}},
Pages = {{1031-1055}},
Month = {{DEC}},
Abstract = {{This large-scale intermedia agenda-setting analysis examines U.S. online
   media sources for 2015. The network agenda-setting model showed that
   media agendas were highly homogeneous and reciprocal. Online partisan
   media played a leading role in the entire media agenda. Two elite
   newspapersThe New York Times and The Washington Postwere found to no
   longer be in control of the news agenda and were more likely to follow
   online partisan media. This article provides evidence for a nuanced view
   of the network agenda-setting model; intermedia agenda-setting effects
   varied by media type, issue type, and time periods.}},
